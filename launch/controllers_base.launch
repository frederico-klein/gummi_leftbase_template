    <rosparam file="$(eval find('gummi_leftbase_' + base) + '/dynamixel/left_shoulder_roll_encoder.yaml')" command="load"/>
    <node name="left_shoulder_roll_encoder_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_se
                left_shoulder_roll_encoder_controller"
          output="screen"/>

    <rosparam file="$(eval find('gummi_leftbase_' + base) + '/dynamixel/left_shoulder_yaw_encoder.yaml')" command="load"/>
