    <node name="left_horizontal_adductor_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port gummi_d
                left_horizontal_adductor_controller"
          output="screen"/>

    <rosparam file="$(eval find('gummi_leftbase_' + base) + '/dynamixel/left_shoulder_pitch_encoder.yaml')" command="load"/>
    <node name="left_shoulder_pitch_encoder_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
